  <main id="main">
    <!-- PAGE | WELCOME -->
    <div class="pages white page-welcome fullheight flashback" id="page-welcome">
      <div class="center">
        <div class="line-divider" id="banner">
          <h1>!=$</h1>
        </div>
        <h2>MIT Bitcoin Expo 2018</h2>
        <h3>March 17-18 | <a href="http://whereis.mit.edu/?go=26">Room 26-100</a></h3> and <a href="http://whereis.mit.edu/?go=32">32-123</a></h3>

        <div class="social-icons sicon-white bordered">
          <a href="https://www.facebook.com/MITBitcoinClub" class="sicon-facebook"><i>Facebook</i></a>
          <a href="https://twitter.com/mitbitcoinclub" class="sicon-twitter"><i>Twitter</i></a>
          <a href="https://www.youtube.com/user/MITBitcoinClub" class="sicon-youtube"><i>Youtube</i></a>
        </div>
      </div>
    </div>
    <!-- END PAGE | Welcome -->

    <!-- PAGE | COMPETITION -->
    <div class="pages page-competition" id="page-competition">
      <div class="center">
        <div class="container">
          <header id="schedule">
            <div class="line-divider">
              <h1>2018 Highlights</h1>
            </div>
          </header>
          <div class="aspect-ratio">
            <iframe src="https://www.youtube.com/embed/d26r0YDeqq4" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
          </div>
        </div>
      </div>
    </div>
    <!-- END PAGE | COMPETITION -->

    <!-- PAGE | SCHEDULE -->
    <div class="pages white page-schedule" id="page-schedule">
      <div class="container">
        <header id="schedule">
          <h3 class="line-divider">2018 Expo Schedule Day 1</h3>
        </header>
        <h2>26-100</h2>
        <table class="table table-condensed col-md-8">
          <thead>
            <tr>
              <th><h4>Time</h4></th>
              <th><h4>Topic</h4></th>
              <th><h4>Speakers</h4></th>
            </tr>
          </thead>
          <tbody>
            {% for index, [time,event,format,speaker] in enumerate(day1_26) %}
            <tr>
              <td>{{time}}</td>
              <td>{{event}}</td>
              <td>{{speaker}}</td>
            </tr>
            {% end %}
          </tbody>
        </table>
        <h2>32-123</h2>
        <table class="table table-condensed">
          <thead>
            <tr>
              <th><h4>Time</h4></th>
              <th><h4>Topic</h4></th>
              <th><h4>Moderator</h4></th>
              <th><h4>Panelists</h4></th>
            </tr>
          </thead>
          <tbody>
            {% for index, [time,format,event,moderator,panelists] in enumerate(day1_32) %}
            <tr>
              <td>{{time}}</td>
              <td>{{event}}</td>
              <td>{{moderator}}</td>
              <td>{{panelists}}</td>
            </tr>
            {% end %}
          </tbody>
        </table>
          <header id="schedule">
            <h3 class="line-divider">2018 Expo Schedule Day 2</h3>
          </header>
          <h2>26-100</h2>
          <table class="table table-condensed col-md-8">
            <thead>
              <tr>
                <th><h4>Time</h4></th>
                <th><h4>Topic</h4></th>
                <th><h4>Speakers</h4></th>
              </tr>
            </thead>
            <tbody>
              {% for index, [time,event,format,speaker] in enumerate(day2_26) %}
              <tr>
                <td>{{time}}</td>
                <td>{{event}}</td>
                <td>{{speaker}}</td>
              </tr>
              {% end %}
            </tbody>
          </table>
          <h2>32-123</h2>
          <table class="table table-condensed">
            <thead>
              <tr>
                <th><h4>Time</h4></th>
                <th><h4>Topic</h4></th>
                <th><h4>Moderator</h4></th>
                <th><h4>Panelists</h4></th>
              </tr>
            </thead>
            <tbody>
              {% for index, [time,format,event,moderator,panelists] in enumerate(day2_32) %}
              <tr>
                <td>{{time}}</td>
                <td>{{event}}</td>
                <td>{{moderator}}</td>
                <td>{{panelists}}</td>
              </tr>
              {% end %}
            </tbody>
          </table>
      </div>
    </div>
    <!-- END PAGE | SCHEDULE -->

